nums=[]
for i in open('C:\\Users\\Ð¯\Desktop\\pyton(1)\\111.txt'):
   nums.append(i[:-1])
print(nums)
d={c:nums.count(c) for c in nums}
print(d)


mystring=str("eyi")
def ispalindrome(mystring):
    return mystring==mystring[::-1]
print(ispalindrome(mystring))
c='Hello Baby'
m=''
t=''
for i in c:
    if i.isupper():
        m+=i.upper()
    else:
        t+=i.lower()
print(len(m))
print(len(t))
d={'upper':len(m),'lower':len(t)}
print(d)